{
  "RetryPolicy": {
    "RetryCount": 3,
    "InitialDelayMilliseconds": 200
  },
  "PollyPolicies": {
    "TimeoutSeconds": 3,
    "CircuitBreaker": {
      "HandledEventsAllowedBeforeBreaking": 2,
      "DurationOfBreakSeconds": 10
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}